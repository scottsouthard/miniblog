<%= form_for(@user) do |f| %>
  <%= f.label :full_name%>
  <%= f.text_field :full_name, autofocus: true, placeholder: "Enter Name" %>
  <%= f.label :email %>
  <%= f.text_field :email, placeholder: "Enter Email" %>
  <%= f.label :password %>
  <%= f.password_field :password, placeholder: "Enter Password" %>
  <%= f.submit "Register" %>
<% end %>
